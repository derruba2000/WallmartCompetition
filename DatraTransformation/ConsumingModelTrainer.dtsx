    #endregion

    List<ModelLib.ModelOutputExt> predictions;
    /// <summary>
    /// This method is called once, before rows begin to be processed in the data flow.
    ///
    /// You can remove this method if you don't need to do anything here.
    /// </summary>
    public override void PreExecute()
    {
        base.PreExecute();
        /*
         * Add your code here
         */
        Boolean fireAgain = true;

        string product = Variables.product;
        string connectionstring = Variables.connectionString;
        string ModelsPath = Variables.ModelsPath;
        int FutureDays = Variables.FutureDays;
        int PreviousDays = Variables.PreviousDays;
        string CutOffDate = Variables.CutOffDate;

        ComponentMetaData.FireInformation(10, "Process Values 1)", "Creating Predict Object...", "", 0, fireAgain);
        ModelPredict mlPredict = new ModelPredict(connectionstring,
                                                  ModelsPath,
                                                  product);


        ComponentMetaData.FireInformation(10, "Process Values 2)", "Extracting data...", "", 0, fireAgain);
        string strTMP=mlPredict.ExtractData(PreviousDays, FutureDays, CutOffDate);


        ComponentMetaData.FireInformation(10, "Process Values 3)", "Make Predictions...", "", 0, fireAgain);
        this.predictions = mlPredict.PredictData(FutureDays);

      
    }

    /// <summary>
    /// This method is called after all the rows have passed through this component.
    ///
    /// You can delete this method if you don't need to do anything here.
    /// </summary>
    public override void PostExecute()
    {
        base.PostExecute();
        /*
         * Add your code here
         */

        Boolean fireAgain = true;

        ComponentMetaData.FireInformation(10, "Process Values 4)", "Finish Prediciton...", "", 0, fireAgain);


    }

    public override void CreateNewOutputRows()
    {
        /*
          Add rows by calling the AddRow method on the member variable named "<Output Name>Buffer".
          For example, call MyOutputBuffer.AddRow() if your output was named "MyOutput".
        */
        foreach (var pred in this.predictions)
        {
            
            Output0Buffer.AddRow();
            Output0Buffer.ActualSales = (Int32) pred.TotalSales;
            Output0Buffer.ForecastSales = (Int32)pred.ForecastedSales;
            Output0Buffer.UpperBound = (Int32)pred.UpperBoundSales;
            Output0Buffer.LowerBound = (Int32)pred.LowerBoundSales;
            Output0Buffer.Index = (Int32)pred.indexX;
            Output0Buffer.Date = pred.SalesDate;

        }

        
    }

}
